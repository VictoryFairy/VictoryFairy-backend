{
  "name": "VictoryFairy-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "convert-song": "ts-node ./src/tools/seeds/data/cheering-song-converter.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "------------------------------------------ E2E TEST ------------------------------------------": "",
    "e2e:run": "npm run e2e:up && npm run e2e:migration:run && npm run e2e:seed && npm run e2e:test; EXIT_CODE=$?; npm run e2e:down; exit $EXIT_CODE",
    "e2e:test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles --forceExit --testTimeout=30000",
    "e2e:up": "docker compose -f docker-compose.e2e.yml --env-file .env.test up -d",
    "e2e:down": "docker compose -f docker-compose.e2e.yml down",
    "e2e:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./test/utils/test-datasource.ts",
    "e2e:migration:run": "npm run typeorm -- migration:run -d ./test/utils/test-datasource.ts",
    "e2e:migration:revert": "npm run typeorm -- migration:revert -d ./test/utils/test-datasource.ts",
    "------------------------------------------- SEED -------------------------------------------": "",
    "seed:prod": "node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./dist/tools/datasource.js",
    "seed:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./src/tools/datasource.ts",
    "----------------------------------------- MIGRATION -----------------------------------------": "",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create ./src/tools/migrations/schema-update",
    "migration:generate": "npm run typeorm -- migration:generate ./src/tools/migrations/schema-update -d ./src/tools/datasource.ts",
    "migration:show": "npm run typeorm -- migration:show -d ./dist/tools/datasource.js",
    "migration:show:dev": "npm run typeorm -- migration:show -d ./src/tools/datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./dist/tools/datasource.js",
    "migration:run:dev": "npm run typeorm -- migration:run -d ./src/tools/datasource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./dist/tools/datasource.js",
    "migration:revert:dev": "npm run typeorm -- migration:revert -d ./src/tools/datasource.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.623.0",
    "@aws-sdk/client-ses": "^3.787.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/nestjs": "^9.22.0",
    "@slack/webhook": "^7.0.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cron": "^3.1.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "node-html-parser": "^6.1.13",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3",
    "typeorm-transactional": "^0.5.0",
    "ua-parser-js": "^2.0.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
